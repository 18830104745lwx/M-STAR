{
    "task": "traffic_state_pred",
    "model": "TrafficFormer",
    "dataset": "ChengduDiDi20x20",
    
    "embed_dim": 80,
    "skip_dim": 256,
    "geo_num_heads": 4,
    "sem_num_heads": 2,
    "t_num_heads": 2,
    "adp_num_heads": 2,
    "geo_num_groups": 2,
    "sem_num_groups": 1,
    "adp_num_groups": 1,
    "adp_topk": 10,
    "mlp_ratio": 4,
    "qkv_bias": true,
    "drop": 0.0,
    "attn_drop": 0.0,
    "drop_path": 0.3,
    "s_attn_size": 3,
    "t_attn_size": 1,
    "enc_depth": 6,
    "type_ln": "post",
    "type_short_path": "hop",
    
    "scaler": "standard",
    "load_external": true,
    "normal_external": false,
    "ext_scaler": "none",
    "add_time_in_day": true,
    "add_day_in_week": true,
    "input_window": 12,
    "output_window": 1,
    "output_dim": 2,
    "batch_size": 16,
    
    "seed": 0,
    "max_epoch": 100,
    "learner": "adamw",
    "learning_rate": 0.001,
    "weight_decay": 0.05,
    "lr_decay": true,
    "lr_scheduler": "cosinelr",
    "lr_eta_min": 0.0001,
    "lr_decay_ratio": 0.1,
    "lr_warmup_epoch": 5,
    "lr_warmup_init": 1e-06,
    "clip_grad_norm": true,
    "max_grad_norm": 5,
    "use_early_stop": true,
    "patience": 5,
    "use_curriculum_learning": true,
    "random_flip": true,
    "huber_delta": 2.0,
    "quan_delta": 0.25,
    "bidir": false,
    "far_mask_delta": 5,
    "dtw_delta": 5,
    "set_loss": "composite_mae_mape",
    "loss_w_mae": 0.6,
    "loss_w_mape": 0.4,
    "metrics": ["MAE", "RMSE", "MAPE", "R2"],
    
    "use_swiglu_ffn": true,
    "use_time_guide": true,
    "time_guide_dilations": [1, 2, 4, 8],
    
    "gpu": true,
    "gpu_id": [0],
    "train": true,
    "cache_dataset": true,
    "num_workers": 0,
    "pad_with_last_sample": true,
    "train_rate": 0.7,
    "eval_rate": 0.1,
    "lape_dim": 8
}

